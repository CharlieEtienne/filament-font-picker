{
    "name": "charlieetienne/filament-font-picker",
    "description": "A Filament form component for selecting Google Fonts with real-time previews, search functionality, and category filtering.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "filament",
        "laravel",
        "font-picker",
        "google-fonts",
        "form-component"
    ],
    "authors": [
        {
            "name": "Charlie Etienne",
            "email": "hello@charlieetienne.com"
        }
    ],
    "homepage": "https://github.com/charlieetienne/filament-font-picker",
    "support": {
        "issues": "https://github.com/charlieetienne/filament-font-picker/issues",
        "source": "https://github.com/charlieetienne/filament-font-picker"
    },
    "require": {
        "php": "^8.2",
        "filament/forms": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/framework": "^11.0",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "CharlieEtienne\\FilamentFontPicker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CharlieEtienne\\FilamentFontPicker\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CharlieEtienne\\FilamentFontPicker\\FontPickerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
